<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Border Top & Bottom-->
    <item>
        <shape >
            <solid android:color="@color/tab_home_button_border_bottom"/>
        </shape>
    </item>

    <!-- Border Left-->
    <item
        android:top="0dip"
        android:left="0dip"
        android:right="0dip"
        android:bottom="1dip"
        >
        <shape >
            <solid android:color="@color/tab_home_button_border_left_p"/>
        </shape>
    </item>

    <!-- Border Right-->
    <item
        android:top="1dip"
        android:left="1dip"
        android:right="0dip"
        android:bottom="1dip"
        >
        <shape >
            <solid android:color="@color/tab_home_button_border_right_p"/>
        </shape>
    </item>

    <!--Button-->
    <item
        android:left="1dip"
        android:right="1dip"
        android:bottom="1dip"
        android:top="1dip"
        >
        <shape >
            <solid android:color="@color/tab_home_button_c" />
        </shape>
    </item>



</layer-list>