<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		>
		<shape
			android:shape="oval">
			<size android:height="@dimen/player_indicator_size"
				  android:width="@dimen/player_indicator_size"/>
			<solid android:color="@color/player_ind_black_stroke"/>
		</shape>
	</item>
	<item
		android:top="1px"
		android:bottom="1px"
		android:left="1px"
		android:right="1px"
		>
		<shape
			android:shape="oval">
			<solid android:color="@color/player_ind_black_2"/>
		</shape>
	</item>
	<item
		android:top="2px"
		android:bottom="0px"
		android:left="2px"
		android:right="0px"
		>
		<shape
			android:shape="oval">
			<size android:height="@dimen/player_indicator_size1"
				android:width="@dimen/player_indicator_size1"/>
			<gradient
				android:angle="270"
				android:startColor="@color/player_ind_black_1"
				android:endColor="@color/player_ind_black_1"
				/>

			<stroke android:color="@color/player_ind_black_stroke"
					android:width="1px"/>
		</shape>
	</item>

</layer-list>
