<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    
    <item
		android:left="2dip"
		android:right="2dip"
        >

        <shape>
            <corners android:radius="@dimen/seek_radius" />
            <solid android:color="@color/seek_background" />
			<stroke android:color="@color/seek_background_stroke"
				android:width="@dimen/seek_stroke_width"/>
            <size android:height="20dip" />
        </shape>
    </item>

    <item
        android:left="6dip"
        android:right="6dip"
        android:top="6dip"
        android:bottom="6dip"
        android:id="@android:id/background"
        android:drawable="@drawable/progress_seek_temp">

        <shape>
            <corners android:radius="@dimen/seek_radius" />
            <solid android:color="@color/seek_background" />
            <stroke android:color="@color/seek_background_stroke"
                    android:width="@dimen/seek_stroke_width"/>
            <size android:height="20dip" />
        </shape>
    </item>
    <item
		android:left="6dip"
		android:right="6dip"
		android:id="@android:id/secondaryProgress">
        <clip android:drawable="@drawable/empty">

        </clip>
    </item>
    
    <item
		android:left="6dip"
		android:right="6dip"
		android:id="@android:id/progress" >
        <clip android:drawable="@drawable/empty" >

        </clip>
    </item>
    
</layer-list>

