<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

	<!--Border-->
	<item
		android:top="1dp"
		android:left="1dp"
		android:right="1dp"
		android:bottom="1dp">
		<shape android:shape="oval">
			<solid android:color="@color/thumbnail_play_border_medium_p" />
			<size
				android:width="@dimen/video_play_btn_back_ring_size_medium"
				android:height="@dimen/video_play_btn_back_ring_size_medium" />

		</shape>
	</item>

	<!--Inner Ring-->
	<item
		android:top="2dp"
		android:left="2dp"
		android:right="2dp"
		android:bottom="2dp">
		<shape android:shape="oval">
			<solid android:color="@color/thumbnail_play_back_medium_p" />
		</shape>
	</item>


</layer-list>